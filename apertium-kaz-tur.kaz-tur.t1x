<rule comment="REGLA: pres">
       <pattern>
        <pattern-item n="pres"/>    
      </pattern>
      <action>
        <let>
          <clip pos="1" side="tl" part="a_pers_nbr"/>
          <concat><lit v="+i"/><lit-tag v="cop.aor"/><clip pos="1" side="tl" part="a_pers_nbr"/></concat>
        </let>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>






 <rule comment="REGLA: prc_perf+yat_pres">
      <pattern>
        <pattern-item n="prc_perf"/>
        <pattern-item n="yat_pres"/>
      </pattern>
      <action>
<let>
<clip pos="1" side="tl" part="a_aspect"/><lit-tag v="prog"/>
</let>
        <let>
          <clip pos="1" side="tl" part="whole"/>

          <concat><clip pos="1" side="tl" part="whole"/><lit v="+i"/><lit-tag v="cop.aor"/><clip pos="2" side="tl" part="a_pers_nbr"/></concat>
        </let>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>


<rule comment="REGLA: prc_perf+otyr_pres">
      <pattern>
        <pattern-item n="prc_perf"/>
        <pattern-item n="otyr_pres"/>
      </pattern>
      <action>
<let>
<clip pos="1" side="tl" part="a_aspect"/><lit-tag v="prog"/>
</let>
        <let>
          <clip pos="1" side="tl" part="whole"/>

          <concat><clip pos="1" side="tl" part="whole"/><lit v="+i"/><lit-tag v="cop.ifi"/><clip pos="2" side="tl" part="a_pers_nbr"/></concat>
        </let>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>



<rule comment="REGLA: cop">
      <pattern>
      <pattern-item n="adj"/>
        <pattern-item n="cop"/>
      </pattern>
      <action>

<choose>
   <when>
     
      <test><equal><clip pos="2" side="tl" part="evid"/><lit-tag v="aor.evid"/></equal></test>
                             
        
      <let><clip pos="2" side="tl" part="evid"/><lit-tag v="dub.aor"/></let>
    </when>
</choose>

        <out>

          <chunk name="v" case=
          "caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
          <mlu>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
             </lu> 
             <lu>
              <clip pos="2" side="tl" part="whole"/>
            </lu>
          </mlu>
          
          </chunk>
        </out>
      </action>
    </rule>


<rule comment="REGLA: prc_vol+кел">
      <pattern>
      <pattern-item n="prc_vol"/> 
      <pattern-item n="кел"/>
      </pattern>
      <action>



        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
         
            <lu>
              <clip pos="1" side="tl" part="lem"/><clip pos="1" side="tl" part="POS"/><clip pos="1" side="tl" part="V_trans"/><lit-tag v="ger_inf"/><lit-tag v="nom"/></lu>
             <b/> 
             <lu>
              <clip pos="2" side="tl" part="lem"/><clip pos="2" side="tl" part="POS"/><clip pos="2" side="tl" part="V_trans"/><clip pos="2" side="tl" part="neg"/><clip pos="2" side="tl" part="tenses"/><clip pos="1" side="tl" part="a_pers_nbr"/>
            </lu>
          
          </chunk>
        </out>
      </action>
    </rule>




 <rule comment="REGLA: SENT">
      <pattern>
        <pattern-item n="cop-neg"/>
      </pattern>
      <action>
        <out>
          <chunk name="sent" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SENT"/></tag>
            </tags>
            <lu>
<lit v="değil"/><lit-tag v="adj"/>
</lu>
          </chunk>
        </out>
      </action>
    </rule>


    <rule comment="REGLA: SENT">
      <pattern>
        <pattern-item n="sent"/>
      </pattern>
      <action>
        <out>
          <chunk name="sent" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SENT"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

  </section-rules>
</transfer>

