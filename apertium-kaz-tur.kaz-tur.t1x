<section-def-vars> 
  <def-var n="number"/>
  </section-def-vars>

<section-def-macros>
    <def-macro n="test" npar="1">
          <let>
            <var n="number"/>
            <lit v=""/>
          </let>
    </def-macro>
    
  </section-def-macros>
  
  

<section-rules>

<rule comment="REGLA: AOR" >
      <pattern>
        <pattern-item n="aor"/>
      </pattern>
      <action>

<choose>
   <when>

     <test><equal><clip pos="1" side="tl" part="a_pers_nbr"/> 
                             <lit-tag v="p2.frm.sg"/></equal></test>
  
     <let><clip pos="1" side="tl" part="a_pers_nbr"/><lit-tag v="p2.pl"/></let>
    </when>
</choose>
<let>
<clip pos="1" side="tl" part="a_aspect"/><lit-tag v="prog"/>
</let>
        <let>
          <clip pos="1" side="tl" part="a_pers_nbr"/>
          <concat><lit v="+i"/><lit-tag v="cop.aor"/><clip pos="1" side="tl" part="a_pers_nbr"/></concat>
        </let>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
    
    
    
  <rule comment="REGLA: nom+prn">

     <pattern>
     
     
        <pattern-item n="prn"/>
        <pattern-item n="nom"/>
       </pattern>

<action>

        <let>
        
          <clip pos="2" side="tl" part="diff.name"/>
          <lit v=""/>
        </let>

<out>
     <chunk name="n" case="caseFirstWord">
          <tags>
              <tag><lit-tag v="SN"/></tag>
            </tags>
          
            <lu>
            <clip pos="1" side="tl" part="whole"/>
          
          </lu>
          <b/>
           <lu>
            <clip pos="2" side="tl" part="whole"/>
          
          </lu>
          </chunk>
        </out>
      </action>
    </rule>
    
    
<rule comment="REGLA: nom">

     <pattern>
     
     
        <pattern-item n="prn"/>
        <pattern-item n="nom"/>
       </pattern>

<action>
<choose>
   <when>

     <test><equal><clip pos="1" side="tl" part="a_pers_nbr"/> 
                             <lit-tag v="p1.pl"/></equal></test>
  
     <let><clip pos="1" side="tl" part="a_pers_nbr"/><lit-tag v="px1pl"/></let>
    </when>
</choose>
<let>
          <clip pos="2" side="tl" part="whole"/>
          <concat><clip pos="2" side="tl" part="lem"/><lit-tag v="n"/><clip pos="1" side="tl" part="a_pers_nbr"/><lit-tag v="nom"/></concat>
        </let>


<out>
     <chunk name="n" case="caseFirstWord">
          <tags>
              <tag><lit-tag v="SN"/></tag>
            </tags>
          
            <lu>
            <clip pos="2" side="tl" part="whole"/>
          
          </lu>
          </chunk>
        </out>
      </action>
    </rule>
    
    
<rule comment="REGLA: imp-pl">

     <pattern>
        <pattern-item n="imp"/>
       </pattern>
  


<action>
      <choose>       
          <when>   
            <test>
               <and>
                 <equal>
                    <clip pos="1" side="sl" part="personne"/>
                    <lit-tag v="p2"/>
                  </equal>
                 <equal>
                    <clip pos="1" side="sl" part="nombre"/>
                    <lit-tag v="sg"/>
                  </equal>
                </and>
            </test>
            

            <let>
             <clip pos="1" side="tl" part="personne"/>
                    <lit-tag v="p1"/>
            </let>
            <let>
              <clip pos="1" side="tl" part="nombre"/>
              <lit-tag v="pl"/>
            </let>
          </when>
        </choose>
 
        <out>
          <chunk name="v" case="caseFirstWord">
          <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
          <lu> 
            <clip pos="1" side="tl" part="lem"/>
            <clip pos="1" side="tl" part="v_iv"/>
            <lit-tag v="imp"/>
          
            <clip pos="1" side="tl" part="personne"/>
            <clip pos="1" side="tl" part="nombre"/>
          </lu>
          </chunk>
        </out>
      </action>
    </rule>
    
    
